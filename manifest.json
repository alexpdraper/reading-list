{
  "manifest_version": 2,

  "name": "Reading List",
  "description": "This extension saves a list of links to pages to read later",
  "version": "2.3.1",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_popup": "html/popup.html"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "https://ajax.googleapis.com/",
    "contextMenus",
    "storage"
  ]
}
